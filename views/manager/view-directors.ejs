<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/header'); -%>
    <link rel="stylesheet" href="/css/table.css">
</head>

<body>
    <table>
        <tr>
          <th>Username</th>
          <th>Name</th>
          <th>Surname</th>
          <th>Nation</th>
          <th>Platform ID</th>
        </tr>
        <% for (var i = 0; i < directors.length; i++) { %>
            <tr>
                <td><%= directors[i].username %></td>
                <td><%= directors[i].name %></td>
                <td><%= directors[i].surname %></td>
                <td><%= directors[i].nation %></td>
                <td><%= directors[i].platform_id %></td>
            </tr>
        <% } %>
      </table>
</body>
</html>
